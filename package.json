{
  "name": "matrix-strava-bot",
  "version": "0.1.0",
  "description": "A chatbot that reports Strava club activities",
  "main": "lib/index.js",
  "scripts": {
    "// ------ DEVELOPMENT ----- //": "",
    "dev": "nodemon src/index.ts --dryrun",
    "convert": "ts-jest config:migrate ./jest.config.js",
    "// ------ TESTING ----- //": "",
    "test": "jest --watch",
    "coverage": "jest --collectCoverageFrom=src/**/*.ts --coverage",
    "// ------ BUILDING ----- //": "",
    "build": "tsc",
    "build:watch": "tsc -w",
    "// ------ LINTING ----- //": "",
    "lint": "eslint . --ext .ts",
    "// ------ PRODUCTION ----- //": "",
    "start": "ts-node src/index.ts"
  },
  "keywords": [
    "Matrix",
    "Bot",
    "Strava"
  ],
  "author": "Nik0kin",
  "license": "ISC",
  "dependencies": {
    "matrix-bot-sdk": "^0.5.6",
    "strava-v3": "^2.0.5"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.2.5",
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@types/jest": "^24.0.12",
    "@types/node": "^11.10.5",
    "@types/node-fetch": "^2.5.7",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@typescript-eslint/parser": "^1.4.2",
    "eslint": "^5.15.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-prettier": "^3.0.1",
    "jest": "^24.3.0",
    "nodemon": "^1.18.10",
    "prettier": "^1.16.4",
    "ts-jest": "^26.3.0",
    "ts-node": "^8.0.3",
    "typescript": "^3.3.3333"
  }
}